<p id="notice"><%= notice %></p>

<h1><%= @user.name %></h1>


<% @user.pics.each do |pic| %>
  <h4><%=pic.title%></h4>
  <% if pic.photo.present? %>
    <p><%= image_tag pic.photo.url(:thumb) %></p>
  <% end %>
<% end %>
<% if @user == current_user %>
  <%= link_to 'Edit', edit_user_path(@user) %> |
<% end %>

<%= link_to 'Back', ('/pics') %>
